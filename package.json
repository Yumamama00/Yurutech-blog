{
  "name": "yurutech-blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "npm run sitemap && npm run rss && next export",
    "sitemap": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-sitemap.mjs",
    "rss": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-rss.mjs",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.7",
    "contentlayer": "latest",
    "esbuild": "^0.14.43",
    "github-slugger": "^1.4.0",
    "gray-matter": "^4.0.3",
    "image-size": "1.0.1",
    "mdx-bundler": "^9.0.1",
    "next": "12.1.6",
    "next-contentlayer": "latest",
    "next-themes": "^0.2.0",
    "postcss": "^8.4.14",
    "preact": "^10.7.3",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-citation": "^0.4.0",
    "rehype-katex": "^6.0.2",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.4.0",
    "rehype-slug": "^5.0.1",
    "remark": "^14.0.2",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "@heroicons/react": "^1.0.6",
    "tailwindcss": "^3.1.2",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.1.6",
    "@svgr/webpack": "^6.2.1",
    "@types/react": "^18.0.12",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "eslint": "^8.17.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "file-loader": "^6.2.0",
    "globby": "13.1.1",
    "husky": "^8.0.1",
    "inquirer": "^8.2.4",
    "lint-staged": "^13.0.1",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "typescript": "^4.7.3"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
